node{
    stage("build"){
        bat "dotnet build --configuration Release"
    }
    stage("publish"){
        bat "dotnet publish -r win-x64 --self-contained"
    }
    stage("capture"){
        archiveArtifacts artifacts: '**/Release/net7.0/**'
    }
    stage("deploy"){
        bat "Xcopy WebApplication1\\bin\\Release\\net7.0\\ c:\\inetpub\\wwwroot\\TestWebApp /E /H /C /I /Y"
    }
}